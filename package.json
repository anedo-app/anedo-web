{
  "name": "anedo-web",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && npx stylelint '**/*.module.scss'",
    "lint:fix": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky",
    "test:unit": "vitest",
    "icons": "node ./scripts/fetch-icons.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext '.ts,.tsx' --fix"
    ],
    "*.module.scss": [
      "npx stylelint"
    ]
  },
  "dependencies": {
    "date-fns": "^3.6.0",
    "firebase": "^10.12.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "nanoid": "^5.0.7",
    "node-sass": "^9.0.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@testing-library/react": "^15.0.2",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.set": "^4.3.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-pretty-imports": "^1.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.39",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "semantic-release": "^22.0.12",
    "storybook": "^8.0.9",
    "stylelint": "^16.3.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vitest": "^1.5.0"
  }
